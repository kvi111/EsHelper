    <Grid>
        <!--<ScrollViewer>-->
            <TextBox Name="tbMultiLine" TextWrapping="Wrap" AcceptsReturn="True" Height="600" Width="450" ScrollViewer.VerticalScrollBarVisibility="Visible">
            </TextBox>
        <!--</ScrollViewer>-->
    </Grid>
</ContentDialog>
